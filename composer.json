{
    "name": "bchubbweb/phntm_framework",
    "description": "a basic framework build from symfony, with filesystem based routing",
    "type": "project",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Pages\\": "pages/",
            "Bchubbweb\\PhntmFramework\\": "phntm/"
        }
    },
    "authors": [
        {
            "name": "Billy Chubb",
            "email": "billy@bchubb.co.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/http-foundation": "^7.0",
        "symfony/routing": "^7.0",
        "twig/twig": "^3.0",
        "predis/predis": "^2.2",
        "vlucas/phpdotenv": "^5.6",
        "relay/relay": "^2.1",
        "maximebf/debugbar": "^1.22",
        "symfony/psr-http-message-bridge": "^7.0",
        "nyholm/psr7": "^1.8",
        "middlewares/debugbar": "^2.1",
        "laminas/laminas-httphandlerrunner": "^2.10",
        "middlewares/whoops": "^2.0"
    }
}
